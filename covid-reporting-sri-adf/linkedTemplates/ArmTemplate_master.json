{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-reporting-sri-adf"
		},
		"ls_ablob_covidreportingsasri_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportingsasri'"
		},
		"ls_adls_covidreportingdlsri_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingdlsri'"
		},
		"ls_azure_sql_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azure_sql_db'"
		},
		"ls_adls_covidreportingdlsri_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingdlsri.dfs.core.windows.net"
		},
		"ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().SourceBaseURL}"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/7dfa7c73-f1ac-4cc1-b312-baff36533ac8/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/covidreportingsasri"
		},
		"trigger1_properties_pipeline1_parameters_num": {
			"type": "int",
			"defaultValue": 45
		},
		"trigger1_properties_pipeline1_parameters_denom": {
			"type": "int",
			"defaultValue": 15
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-reporting-sri-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportingsasri_connectionString": {
						"value": "[parameters('ls_ablob_covidreportingsasri_connectionString')]"
					},
					"ls_adls_covidreportingdlsri_accountKey": {
						"value": "[parameters('ls_adls_covidreportingdlsri_accountKey')]"
					},
					"ls_azure_sql_db_connectionString": {
						"value": "[parameters('ls_azure_sql_db_connectionString')]"
					},
					"ls_adls_covidreportingdlsri_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingdlsri_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_europa_eu_properties_typeProperties_url')]"
					},
					"trigger1_properties_pipeline1_parameters_num": {
						"value": "[parameters('trigger1_properties_pipeline1_parameters_num')]"
					},
					"trigger1_properties_pipeline1_parameters_denom": {
						"value": "[parameters('trigger1_properties_pipeline1_parameters_denom')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covid-reporting-sri-adf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covid-reporting-sri-adf_ArmTemplate_0')]"
			]
		}
	]
}